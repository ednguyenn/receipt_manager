                    "type": "aws_proxy"
                },
                "responses": {
                    "200": {
                        "description": "Receipt upload successful"
                    }
                }
            }
        },
        "/search": {
            "post": {
                "x-amazon-apigateway-integration": {
                    "uri": "arn:aws:apigateway:<your-region>:lambda:path/2015-03-31/functions/arn:aws:lambda:<your-region>:<your-account-id>:function:search_receipts_llm/invocations",
                    "httpMethod": "POST",
                    "type": "aws_proxy"
                },
                "responses": {
                    "200": {
                        "description": "Search successful"
                    }
                }
            }
        },
        "/login": {
            "post": {
                "x-amazon-apigateway-integration": {
                    "uri": "arn:aws:apigateway:<your-region>:lambda:path/2015-03-31/functions/arn:aws:lambda:<your-region>:<your-account-id>:function:login/invocations",
                    "httpMethod": "POST",
                    "type": "aws_proxy"
                },
                "responses": {
                    "200": {
                        "description": "Login successful"
                    }
                }
            }
        }
