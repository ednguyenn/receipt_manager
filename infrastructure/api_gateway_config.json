        "title": "Receipt Management API"
    },
    "paths": {
        "/upload": {
            "post": {
                "x-amazon-apigateway-integration": {
                    "uri": "arn:aws:apigateway:<your-region>:lambda:path/2015-03-31/functions/arn:aws:lambda:<your-region>:<your-account-id>:function:process_receipt/invocations",
